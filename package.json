    "apollo-live-server": "^0.2.0",
    "apollo-morpher": "^0.1.2",
    "apollo-server-express": "^1.4.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "casual": "^1.5.19",
    "chart.js": "^2.8.0",
    "classnames": "^2.2.6",
    "draft-js": "^0.10.5",
    "draftjs-to-html": "^0.8.4",
    "easify": "^0.1.0",
    "event-emitter": "^0.3.5",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-load": "^0.1.1",
    "graphql-subscriptions": "^0.5.8",
    "graphql-tag": "^2.9.2",
    "graphql-tools": "^3.0.5",
    "graphql-type-json": "^0.2.1",
    "indexof": "0.0.1",
    "meteor-apollo-accounts": "^2.0.3",
    "meteor-node-stubs": "^0.4.1",
    "mobx": "^5.0.3",
    "mobx-react": "^5.2.3",
